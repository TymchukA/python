def scalar_product(matrix,n):
    result = []
    k=0
    for i in range(0, n):
        result.append([])
        for j in range(0, n):
            sum = 0
            for k in range(0, n):
                sum += matrix[i][k] * matrix[k][j]
                    
            result[i].append(sum)
    return result

